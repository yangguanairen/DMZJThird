<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">

            <size
                android:width="200dp"
                android:height="200dp" />

            <stroke android:color="@color/icon_color"
                android:width="2dp"
                android:dashWidth="@dimen/dp_10"
                android:dashGap="@dimen/dp_10"/>

            <solid
                android:color="@color/layout_bg" />

        </shape>
    </item>

    <item android:gravity="center" android:drawable="@drawable/ic_add_white"
        android:width="100dp" android:height="100dp" />

</layer-list>